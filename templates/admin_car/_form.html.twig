
{{ form_start(form) }}
<div class="container my-5">
    <div class="row justify-content-center">
        <div class="col-8 col-md-4">
        {{ form_row(form.name) }}
        {{ form_row(form.cost) }}
        </div>
        <div class="col-8 col-md-4">
        {{ form_row(form.category) }}
        {{ form_row(form.nbSeats) }}
        {{ form_row(form.nbDoors) }}
        </div>
    </div>
    {{ form_widget(form) }}
    <div class="d-flex justify-content-center">
    <button class="btn btn-success">{{ button_label|default('Enregistrer') }} <i class="bi bi-check-lg"></i></button>
    </div>
</div>
{{ form_end(form) }}
